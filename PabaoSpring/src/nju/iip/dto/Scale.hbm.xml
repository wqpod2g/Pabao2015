<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nju.iip.dto.Scale" table="parkinsontotalscale">
		<meta attribute="class-description">
			This class contains the nju.iip.dto.Scale detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="scaleName" column="scaleName" type="string" />
		<property name="scaleDescription" column="scaleDescription" type="string" />
		<property name="shortname" column="shortname" type="string" />
	</class>
</hibernate-mapping>