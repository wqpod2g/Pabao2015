<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nju.iip.dto.Comment" table="weixin_comment">
		<meta attribute="class-description">
			This class contains the nju.iip.dto.Comment detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="increment" />
		</id>
		<property name="comment" column="comment" type="string" />
		<property name="postId" column="postId" type="int" />
		<property name="author" column="author" type="string" />
		<property name="openId" column="openId" type="string" />
		<property name="headImgUrl" column="headImgUrl" type="string" />
		<property name="commentTime" column="commentTime" type="string" />
	</class>
</hibernate-mapping>