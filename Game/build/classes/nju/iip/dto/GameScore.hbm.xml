<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nju.iip.dto.GameScore" table="game_score">
		<meta attribute="class-description">
			This class contains the nju.iip.dto.GameScore detail.
		</meta>
		<id name="openId" type="string" column="openId">
			<generator class="assigned" />
		</id>
		<property name="game" column="game" type="string" />
		<property name="nickname" column="nickname" type="string" />
		<property name="score" column="score" type="int" />
		<property name="headImgUrl" column="headImgUrl" type="string" />
	</class>
</hibernate-mapping>